{
	"manifest_version": 2,
	"name": "Coding Train Extention",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [ "content.js" ]
		}
	],
	"background": {
		"persistent": false,
		"scripts": [ "background.js" ]
	},
	"icons": {
		"16": "images/get_started16.png",
		"32": "images/get_started32.png",
		"48": "images/get_started48.png",
		"128": "images/get_started128.png"
	},
	"browser_action": {
		"default_icon": "images/get_started16.png"
	}
}